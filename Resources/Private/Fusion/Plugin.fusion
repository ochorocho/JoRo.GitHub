# Configure Plugin for NEOS ... package, controller and action
prototype(JoRo.GitHub:Plugin) < prototype(Neos.Neos:Plugin)
prototype(JoRo.GitHub:Plugin) {
    package = 'JoRo.GitHub'
    controller = 'Standard'
    action = 'index'
    id = ${node.identifier}
    settings {
        huhu = 'asdsad'
    }
}

