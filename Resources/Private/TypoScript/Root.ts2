# Auto include into any sites package
page {
    head {
        stylesheets.GitHubDetails = TYPO3.TypoScript:Tag {
            @position = 'end 10'
            tagName = 'link'
            attributes {
                media = 'all'
                rel = 'stylesheet'
                href = TYPO3.TypoScript:ResourceUri {
                    path = 'resource://JoRo.GitHub/Public/Styles/Style.css'
                }
            }
        }
    }
}

